{
  "ci": {
    "collect": {
      "numberOfRuns": 3,
      "url": [
        "https://salvatore251086.github.io/ricette-lista-spesa/"
      ],
      "settings": {
        "preset": "desktop",
        "formFactor": "desktop",
        "screenEmulation": { "mobile": false, "width": 1350, "height": 940, "deviceScaleFactor": 1, "disabled": false },
        "locale": "it",
        "onlyCategories": ["performance", "accessibility", "best-practices", "seo"],
        "throttlingMethod": "simulate"
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", { "minScore": 0.90 }],
        "categories:accessibility": ["warn", { "minScore": 0.95 }],
        "categories:best-practices": ["warn", { "minScore": 0.90 }],
        "categories:seo": ["warn", { "minScore": 0.90 }],
        "first-contentful-paint": ["warn", { "maxNumericValue": 2000, "aggregationMethod": "median" }],
        "largest-contentful-paint": ["warn", { "maxNumericValue": 2500, "aggregationMethod": "median" }],
        "total-blocking-time": ["warn", { "maxNumericValue":]()
